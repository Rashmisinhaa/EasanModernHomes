<script>
    var modal = document.getElementById("myModal");
    var baseurl = 'https://magickhome.com/india/sendotp.php';
    // window.onclick = function(event) {
    //     if (event.target == modal) {
    //         modal.style.display = "none";
    //     }
    // }

    function closepopup() {
        modal.style.display = "none";
    }
    const otpInputs = document.querySelectorAll('.otp-input');
    otpInputs.forEach((input, index) => {
        input.addEventListener('input', (event) => {
            // Move to the next input if a digit is entered
            const currentInput = event.target;
            if (isNaN(currentInput.value) || currentInput.value === '') {
                currentInput.value = ''; // Clear non-numeric input
                return;
            }
            if (currentInput.value.length === 1 && index < otpInputs.length - 1) {
                otpInputs[index + 1].focus();
            }
        });

        input.addEventListener('keydown', (event) => {
            // Move to the previous input when backspace is pressed and the input is empty
            if (event.key === 'Backspace' && input.value === '' && index > 0) {
                otpInputs[index - 1].focus();
            }
        });
    });

    function checkEnter(event) {
        if (event.key === 'Enter') {
            verifyOTP();
        }
    }

    function sendOTP() {
        // console.log('sendOTP function triggered')
        document.getElementById('errordiv').style.display = 'none';
        const otpInputs = document.querySelectorAll('.otp-input');
        otpInputs.forEach(input => {
            input.value = '';
            input.style.borderColor = '#999999';
        });
        stopOTPTimer();
        startOTPTimer(180);
        var mobileNumber = document.getElementById('mobileNumber').value;
        if (mobileNumber.length < 10) {
            document.getElementById("myModal").style.display = 'none';
            return;
        }
        // console.log(mobileNumber);
        let type = 'sendotp';
        $.ajax({
            type: "POST",
            url: baseurl,
            data: {
                mobileno: mobileNumber,
                type: type,
            },
            success: function(response) {
                // console.log(response + ' ------response');
                var response = JSON.parse(response);
                // console.log(response.status);
                if (response.status == 'success') {
                    document.getElementById('myModal').style.display = 'block';
                    document.getElementById("otp-firstinput").focus();
                    document.getElementById('otp-phoneno').innerHTML = '+91 ' + mobileNumber;
                } else {
                    document.getElementById('myModal').style.display = 'none';
                    updatePopupContent('Sorry', response.msg);
                    showPopup();
                }
            },
            error: function(xhr, status, error) {
                console.log("Ajax error details:");
                console.log("Status:", status); // Logs the status (e.g., "error", "timeout")
                console.log("XHR object:", xhr); // Logs the full XHR object for more details
                console.log("Error:", error); // Logs the error message (if any)
            }
        });
    }
    let timerInterval;

    function stopOTPTimer() {
        clearInterval(timerInterval);
    }

    function startOTPTimer(duration) {
        const otptimer = document.getElementById("otptimer");
        if (otptimer) {
            otptimer.style.display = 'inline';
        }
        const resendotpbtn = document.getElementById("otpresend");
        if (resendotpbtn) {
            resendotpbtn.classList.remove('enable-resendotp');
        }
        let timer = duration;


        timerInterval = setInterval(() => {

            const minutes = Math.floor(timer / 60);
            const seconds = timer % 60;

            // Display time in MM:SS format
            otptimer.textContent = `(${minutes.toString().padStart(2, '0')}:${seconds.toString().padStart(2, '0')})`;

            // Stop timer when it reaches zero
            if (--timer < 0) {
                clearInterval(timerInterval);
                otptimer.style.display = 'none';
                if (resendotpbtn) {
                    resendotpbtn.classList.add('enable-resendotp');
                }
            }
        }, 1000);
    }


    function verifyOTP() {
        var otp = '';
        var verifyotpbtn = document.getElementById('verifyotpbtn');
        var otpInputs = document.querySelectorAll('.otp-input');
        var mobileNumber = document.getElementById('mobileNumber').value;
        var lsqid = document.getElementById('lsqid').value;
        var path = document.getElementById('redirectionpath').value
        otpInputs.forEach(input => {
            otp += input.value;
        });
        if (otp.length == 4) {
            verifyotpbtn.innerHTML = 'Verifying';
            verifyotpbtn.disabled = true;
            // console.log(otp)
            let type = 'verifyotp';
            $.ajax({
                type: "POST",
                url: baseurl,
                data: {
                    mobileno: mobileNumber,
                    type: type,
                    otp: otp,
                    lsqid: lsqid,
                    path: path
                },
                success: function(response) {
                    // console.log(response);
                    var response = JSON.parse(response);
                    // console.log(response.status);
                    if (response.status == 'success') {
                        document.getElementById('otp-verifiedphoneno').innerHTML = '+91 ' + mobileNumber;
                        document.getElementById('errordiv').style.display = 'none';
                        verifyotpbtn.innerHTML = '<img src="https://www.magickhome.com/india/assets/img/otppopup/verifiedicon.svg" alt="verifyicon" class="verifyicon">Verified';
                        verifyotpbtn.disabled = true;
                        if (path == 'quote-generated') {
                            setTimeout(() => {
                                visitSite();
                            }, 500)
                        } else {
                            otpInputs.forEach(input => {
                                input.style.borderColor = '#999999';
                            });
                            setTimeout(() => {
                                document.getElementById('verification').style.display = 'none';
                                document.getElementById('verified').style.display = 'block';
                            }, 500)
                        }

                    } else {
                        // console.log(response.msg)
                        document.getElementById('errordiv').style.display = 'flex';
                        document.getElementById('otperrortxt').innerHTML = response.msg;
                        verifyotpbtn.innerHTML = 'Verify';
                        verifyotpbtn.disabled = false;
                        otpInputs.forEach(input => {
                            input.style.borderColor = '#FF0000';
                        });

                    }
                },
                error: function(xhr, status, error) {
                    console.log("Ajax error details:");
                    console.log("Status:", status); // Logs the status (e.g., "error", "timeout")
                    console.log("XHR object:", xhr); // Logs the full XHR object for more details
                    console.log("Error:", error); // Logs the error message (if any)
                }
            });

        } else {
            document.getElementById('errordiv').style.display = 'flex';
            document.getElementById('otperrortxt').innerHTML = 'Invalid code';
            verifyotpbtn.innerHTML = 'Verify';
            verifyotpbtn.disabled = false;
            otpInputs.forEach(input => {
                input.style.borderColor = '#FF0000';
            });
        }

    }

    function resendOTP() {
        var otpInputs = document.querySelectorAll('.otp-input');
        var mobileNumber = document.getElementById('mobileNumber').value;
        if (mobileNumber.length < 10) {
            document.getElementById("myModal").style.display = 'none';
            return;
        }
        // console.log(mobileNumber);
        let type = 'resendotp';
        $.ajax({
            type: "POST",
            url: baseurl,
            data: {
                mobileno: mobileNumber,
                type: type,
            },
            success: function(response) {
                // console.log(response);
                var response = JSON.parse(response);
                // console.log(response.status);
                if (response.status == 'success') {
                    startOTPTimer(180);
                    document.getElementById('errordiv').style.display = 'none';
                    document.getElementById('successdiv').style.display = 'block';
                    document.getElementById('otpsuccesstxt').innerHTML = response.msg;

                    verifyotpbtn.innerHTML = 'Verify';
                    otpInputs.forEach(input => {
                        input.style.borderColor = '#999999';
                    });
                    setTimeout(() => {
                        document.getElementById('successdiv').style.display = 'none';
                    }, 1500)
                } else {
                    document.getElementById('errordiv').style.display = 'flex';
                    document.getElementById('otperrortxt').innerHTML = response.msg;
                    document.getElementById('successdiv').style.display = 'none';
                    verifyotpbtn.innerHTML = 'Verify';
                    otpInputs.forEach(input => {
                        input.style.borderColor = '#FF0000';
                    });
                }
            },
            error: function(xhr, status, error) {
                console.log("Ajax error details:");
                console.log("Status:", status); // Logs the status (e.g., "error", "timeout")
                console.log("XHR object:", xhr); // Logs the full XHR object for more details
                console.log("Error:", error); // Logs the error message (if any)
            }
        });
    }

    function visitSite() {
        // alert(document.getElementById('redirectionpath').value);
        var redirection = document.getElementById('redirectionpath').value;
        if (redirection == 'quote-generated') {
            window.location.href = redirection;
        } else {
            window.location.reload();
        }
    }
